<%= render 'header' %>

<h1>Hello World</h1>

<%= render 'flash' %>

<% @articles.each do |article| %>
    <p><%= article.title %></p>
    <p><%= article.content%></p>
    <p><%= link_to 'Show', article_path(article) %></p>
    <p><%= link_to 'Edit', edit_article_path(article) %></p>
    <p><%= link_to 'Delete', article_path(article), method: :delete %></p>
<% end %>